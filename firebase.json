{
  "hosting": {
    "site": "tryingpan-dev",
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/marketplace/details/:pack",
        "destination": "https://xsoar-marketplace.pan.dev/marketplace/details/:pack",
        "type": 301
      },
      {
        "source": "/marketplace/contributors",
        "destination": "https://xsoar-marketplace.pan.dev/marketplace/contributors",
        "type": 301
      }
    ]
  }
}
